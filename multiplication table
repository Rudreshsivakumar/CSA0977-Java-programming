public class MultiplicationTable {
    public static void main(String[] args) {
        int M = 4;
        int N = 5;
        
        printMultiplicationTable(M, N);
    }
    
    public static void printMultiplicationTable(int m, int n) {
        for (int i = 1; i <= n; i++) {
            System.out.println(i + "x" + m + "=" + (i * m));
        }
    }
}
