public class ArrayMatrix {
    public static void main(String[] args) {
        int[] matrixA = new int[9];
        int[] matrixB = new int[9];
        for (int i = 0; i < 9; i++) {
            matrixA[i] = i + 1;
            matrixB[i] = 9 - i;
        }
        System.out.print("Matrix A: ");
        for (int i : matrixA) {
            System.out.print(i + " ");
        }
        System.out.println();
        System.out.print("Matrix B: ");
        for (int i : matrixB) {
            System.out.print(i + " ");
        }
        System.out.println();
    }
}
